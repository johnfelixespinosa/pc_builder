<div class="container">
  <div class="row">
	  <div class="col-md-3">
		  <div class="list-group text-secondary">
        <h1>User Profile</h1>
        <h4><%= @user.username %></h4><br>
        <h2>Account</h2>
        <h6>Builds</h6>
        <h4><%= @user.builds.count %></h4>
        <h6>Parts Lists</h6>
        <h4><%= @user.parts_lists.count %></h4>
        <h6>Comments</h6>
        <h4><%= @user.likes.count %></h4>
        <h6>Member Since</h6>
        <h5><%= @user.created_at.strftime("%b %d, %Y") %></h5>
      </div>
    </div>
    <div class="col-sm-9">
      <nav class="navbar nav nav-tabs justify-content-left">
        <ul class="nav justify-content-end">
          <li class="nav-item">
            <a class="nav-link active" href="#">Parts Lists</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Builds</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Liked Builds</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Comments</a>
          </li>
        </ul>
      </nav>
    </div>
  </div>
</div>